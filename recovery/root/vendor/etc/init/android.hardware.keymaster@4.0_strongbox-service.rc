on property:sys.listeners.registered=true
    start vendor.keymaster-4-0_strongbox

on property:vendor.sys.listeners.registered=true
    start vendor.keymaster-4-0_strongbox

on property:ro.crypto.type=block && property:ro.crypto.fs_crypto_blkdev=*
    stop vendor.keymaster-4-0_strongbox

on property:twrp.all.users.decrypted=true
    stop vendor.keymaster-4-0_strongbox

#service vendor.keymaster-4-0_strongbox /vendor/bin/hw/android.hardware.keymaster@4.0_strongbox-service
    #class early_hal
    #user system
    #group system drmrpc

service vendor.keymaster-4-0_strongbox /vendor/bin/hw/android.hardware.keymaster@4.0_strongbox-service
    user root
    group root
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib:/system/lib64:/system/lib:/sbin
    disabled
    seclabel u:r:recovery:s0
